# The test suite reads parameters from tests.config.  If
# PYTHON_HYPOTHESIS_TESTS_CONFIG is defined, the configuration from
# that file is applied on top of tests.config (items can be added and
# changed, but not removed).  This file is a sample overlay, describing
# all of the possible parameters needed by the test suite.

[TestAPI]
# Use an active authentication token for auth_token and 
# test_authenticated_profile() will check if the api.profile() returns 
# the given userid.
auth_token = some-token
userid = hypuserid

[TestAnnotation]
# Used by test_read() to test the return value of Annotation.load().  If 
# this annotation does not exist (or can not be found), the test results 
# an error.
annotation_id = v8Y0ix_lSRmDnEKhNr19eQ

# eof
